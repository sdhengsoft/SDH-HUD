RemainingTime - SessionTimeLeft;\r\n}\r\n\r\nelse { // Set Number of Laps Session\r\n  var FuelRemainingLaps   = $prop('DataCorePlugin.Computed.Fuel_RemainingLaps');\r\n  var ActualRemainingLaps = $prop('RemainingLaps') - $prop('TrackPositionPercent');\r\n  FuelTest                = FuelRemainingLaps - ActualRemainingLaps;\r\n}\r\n\r\nreturn (FuelTest >=0) ? '#FFCCCCCC' : '#FFFF8C00';"},"Mode":2}}},{"$type":"SimHub.Plugins.OutputPlugins.GraphicalDash.Models.TextItem, SimHub.Plugins","IsTextItem":true,"Font":"Droid Sans","FontSize":16.0,"Text":" Fuel↑","TextColor":"#FFCCCCCC","HorizontalAlignment":1,"VerticalAlignment":1,"BackgroundColor":"#00FFFFFF","Height":20.0,"Left":618.0,"Top":191.0,"Visible":true,"Width":60.0,"Name":"FuelNeededLabel","RenderingSkip":0,"MinimumRefreshIntervalMS":0.0},{"$type":"SimHub.Plugins.OutputPlugins.GraphicalDash.Models.TextItem, SimHub.Plugins","IsTextItem":true,"Font":"Droid Sans","FontSize":20.0,"Text":"-10.0","TextColor":"#FFCCCCCC","HorizontalAlignment":1,"VerticalAlignment":1,"BackgroundColor":"#00FFFFFF","Height":30.0,"Left":618.0,"Top":211.0,"Visible":true,"Width":60.0,"Name":"FuelNeededValue","RenderingSkip":0,"MinimumRefreshIntervalMS":0.0,"Bindings":{"Text":{"FormatString":"","Formula":{"JSExt":0,"Interpreter":1,"Expression":"var FuelNeeded       = 0;\r\nvar FuelLitresPerLap = $prop('DataCorePlugin.Computed.Fuel_LitersPerLap');\r\n\r\nif ( $prop('TotalLaps') == 0) { // Timed Session\r\n  var FuelRemainingTime = timespantoseconds( $prop('DataCorePlugin.Computed.Fuel_RemainingTime'));\r\n  var SessionTimeLeft   = timespantoseconds( $prop('SessionTimeLeft'));\r\n  var BestLapTime       = timespantoseconds( $prop('BestLapTime'));\r\n  var LastLapTime       = timespantoseconds( $prop('LastLapTime'));\r\n\r\n  if ( BestLapTime == 0) BestLapTime = LastLapTime;\r\n  if ( BestLapTime == 0) return '-';\r\n\r\n  FuelNeeded = ( FuelRemainingTime - SessionTimeLeft) / BestLapTime * FuelLitresPerLap;\r\n}\r\n\r\nelse { // Set Number of Laps S